<% @team.users.each do |user| %>
  <!-- if is NOT the owner -->
  <% if !( user.id == @team.rosters.find_by(is_owner: true).user.id ) %>
    <%= user.name %>
    <br>
    <%= image_tag user.image, class: 'user-icon'%>
    <br>
    <% @members.push(user) %>
  <% end %>
<% end %>
